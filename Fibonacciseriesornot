# java-programs
package Numberpatterns;
import java.util.*;
public class Fibonacciornot {
	public static void main(String[] args) {
		 System.out.println("Enter number");
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	int t1=0,t2=1,sum=0;
	for(int i=3;i<=n;i++)
	{
		sum=t1+t2;
		t1=t2;
		t2=sum;
	}
	System.out.print(sum);

	}

}

	
