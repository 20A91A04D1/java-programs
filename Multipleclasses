package Oops;
class Add1 
{
	void add(int a, int b)
	{
		System.out.println("Add is "+(a+b));
	}
}
class Sub1 
{	
	void sub(int x, int y)
	{
		Add1 obj = new Add1();
		obj.add(10,20);
		System.out.println("Sub is "+(x-y));
	}
}

public class File1  
{
	public static void main(String[] args) 
	{
		System.out.println("Main class");
		Sub1 obj2 = new Sub1();
		obj2.sub(33, 11);
	}
}
